    "lint": "tslint -c ./tslint.json --project ./tsconfig.json",
    "test": "jasmine JASMINE_CONFIG_PATH=src/spec/jasmine.config.json || true",
    "watch": "npm run clean && tsc --watch"
  },
  "main": "dist/index.js",
